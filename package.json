{
  "name": "timezone-search",
  "version": "0.1.3",
  "description": "An intelligent, zero-config NPM package for fuzzy searching IANA timezones by city, country, abbreviation, or IANA name. Just install and find the time, anywhere.",
  "main": "index.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "browser": {
        "import": "./browser.mjs",
        "require": "./index.js"
      },
      "node": {
        "import": "./index.mjs",
        "require": "./index.js"
      },
      "import": "./index.mjs",
      "require": "./index.js",
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./browser": "./browser.mjs",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build:data": "node scripts/generate-crossref-timezone-data.js",
    "build:data:clean": "node scripts/generate-clean-timezone-data.js",
    "build:data:legacy": "node scripts/generate-timezone-data.js",
    "test": "jest tests/",
    "test:esm": "node -e \"import('./index.mjs').then(m => { const r = m.search('EST'); console.log('ESM test:', r.length > 0 ? 'PASS' : 'FAIL'); })\"",
    "test:cjs": "node -e \"const {search} = require('./index.js'); const r = search('EST'); console.log('CommonJS test:', r.length > 0 ? 'PASS' : 'FAIL');\"",
    "test:all": "npm test && npm run test:cjs && npm run test:esm",
    "prepublishOnly": "npm run build:data"
  },
  "keywords": [
    "timezone",
    "tz",
    "iana",
    "search",
    "fuzzy",
    "autocomplete",
    "time",
    "world",
    "zone",
    "city",
    "country",
    "typescript",
    "types"
  ],
  "author": "Prashanth Krishnamurthy - techformist.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/timezone-search.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/timezone-search/issues"
  },
  "homepage": "https://github.com/your-username/timezone-search#readme",
  "files": [
    "index.js",
    "index.mjs",
    "browser.mjs",
    "index.d.ts",
    "data/",
    "scripts/",
    "docs/",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "sideEffects": false,
  "dependencies": {
    "fuse.js": "^6.6.2"
  },
  "devDependencies": {
    "countries-and-timezones": "^3.8.0",
    "jest": "^29.7.0",
    "moment-timezone": "^0.6.0"
  }
}
